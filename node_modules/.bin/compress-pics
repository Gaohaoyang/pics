#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/haoyang/Documents/github/pics/node_modules/.pnpm/compress-pics@1.1.7/node_modules/compress-pics/bin/node_modules:/Users/haoyang/Documents/github/pics/node_modules/.pnpm/compress-pics@1.1.7/node_modules/compress-pics/node_modules:/Users/haoyang/Documents/github/pics/node_modules/.pnpm/compress-pics@1.1.7/node_modules:/Users/haoyang/Documents/github/pics/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/haoyang/Documents/github/pics/node_modules/.pnpm/compress-pics@1.1.7/node_modules/compress-pics/bin/node_modules:/Users/haoyang/Documents/github/pics/node_modules/.pnpm/compress-pics@1.1.7/node_modules/compress-pics/node_modules:/Users/haoyang/Documents/github/pics/node_modules/.pnpm/compress-pics@1.1.7/node_modules:/Users/haoyang/Documents/github/pics/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../compress-pics/bin/index.js" "$@"
else
  exec node  "$basedir/../compress-pics/bin/index.js" "$@"
fi
